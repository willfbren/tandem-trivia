{
    "extends": [
      "wesbos"
    ],
    "rules": {
        "indent": ["error", 4],
        "react/prop-types": 0,
        "no-plusplus": 0,
        "prefer-const": "warn",
        "react/button-has-type": 0,
        "jsx-a11y/label-has-associated-control": 0,
        "prettier/prettier": [
            "error",
            {
              "tabWidth": 4,
              "singleQuote": true
            }
          ]
    }
  }